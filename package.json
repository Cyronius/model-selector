{
  "name": "model-selector",
  "version": "0.1.0",
  "description": "Automatic runtime model selection for AI applications using Vercel AI SDK",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./config": {
      "import": "./dist/config/index.js",
      "types": "./dist/config/index.d.ts"
    },
    "./react": {
      "import": "./dist/react/index.js",
      "types": "./dist/react/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "server": "tsx src/server/index.ts",
    "config": "concurrently \"npm:server\" \"npm:config:ui\"",
    "config:ui": "vite",
    "config:build": "vite build"
  },
  "dependencies": {
    "smol-toml": "^1.3.1",
    "zod": "^3.24.0"
  },
  "peerDependencies": {
    "@ai-sdk/anthropic": "^1.0.0",
    "@ai-sdk/azure": "^1.0.0",
    "@ai-sdk/cohere": "^1.0.0",
    "@ai-sdk/google": "^1.0.0",
    "@ai-sdk/groq": "^1.0.0",
    "@ai-sdk/mistral": "^1.0.0",
    "@ai-sdk/openai": "^1.0.0",
    "ai": "^4.0.0",
    "ollama-ai-provider": "^1.0.0",
    "react": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "@ai-sdk/openai": {
      "optional": true
    },
    "@ai-sdk/anthropic": {
      "optional": true
    },
    "@ai-sdk/google": {
      "optional": true
    },
    "@ai-sdk/mistral": {
      "optional": true
    },
    "@ai-sdk/groq": {
      "optional": true
    },
    "@ai-sdk/cohere": {
      "optional": true
    },
    "@ai-sdk/azure": {
      "optional": true
    },
    "ollama-ai-provider": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/react": "^18.0.0",
    "@vitejs/plugin-react": "^4.3.0",
    "concurrently": "^9.2.1",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "tsx": "^4.0.0",
    "typescript": "^5.7.0",
    "vite": "^6.0.0",
    "vitest": "^3.0.0"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "ai",
    "model-selection",
    "vercel-ai-sdk",
    "llm",
    "openai",
    "anthropic"
  ],
  "license": "MIT"
}
